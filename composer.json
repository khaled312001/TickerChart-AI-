{
    "name": "khaled312001/tickerchart-ai",
    "description": "TickerChart AI - Advanced Saudi Stock Market Analysis Platform",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "المهندس خالد أحمد حجاج",
            "email": "developer@example.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TickerChartAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TickerChartAI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "echo 'TickerChart AI installed successfully!'"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 